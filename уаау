#include <stdio.h>
#include <math.h>

int main(){
    int h,g,s=0,k1=0, k2=5;
    int n;
    scanf("%d", &n);


    while (n>0){
        k1 += 1;

        g = (int)pow(10,k2-2);
        h = (int)pow(10,k2-1);
        if (k1 == 1  && (n/g) %10 ==0){
            s+=(n/h)*g;
            k1 += 1;
        }
        else if (k1 %2 != 0 && k1 != 5){
            s += ((n/g) % 10)*h + (n/h)*g;
            printf("%d\n",s);

        }
        else if (k1 %2 != 0){
            s += n % 10;
        }
        n = n - (n /h) * h;
        k2 -= 1;
    }
    printf("%d",s);
}
